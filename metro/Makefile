.PRECIOUS: %.snap %.pbf

%.pbf:
	curl -O http://osm-extracted-metros.s3.amazonaws.com/$@

%.snap: %.pbf
	/data3/data/osm/osmconvert $< | ../snap > $@

%.pnpoly: %.snap
	cat $< | ../pnpoly > $@
